page{
	background-color: #f5f5f5;
}
.section{
	margin-top: 30rpx;
	background-color: #ffffff;
	padding-left: 30rpx;
	border-top: 1rpx solid #EBEEF5;
	border-bottom: 1rpx solid #EBEEF5;
}
.section .item{
	height: 90rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
	padding-right: 30rpx;
}
.section .item-icon{
	width: 30rpx;
	height: 30rpx;
	margin-right: 10rpx;
}
.section .arrow-icon{
	width: 18rpx;
	height: 31rpx;
	margin-left: 10rpx;
}
.section .item-left, .item-right{
	display: flex;
	justify-content: flex-start;
	align-items: center;
}


.logout-btn{
	margin-top: 60rpx;
	background-color: #ffffff;
	height: 90rpx;
	line-height: 90rpx;
	text-align: center;
	border-top: 1rpx solid #EBEEF5;
	border-bottom: 1rpx solid #EBEEF5;
}