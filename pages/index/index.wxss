page{
	background-color: #f5f5f5;
}
.navbar{
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
	padding-left: 30rpx;
	padding-right: 30rpx;
}
.navbar .nav-item{
	background-color: #ffffff;
	display: flex;
	width: 320rpx;
	height: 320rpx;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	margin-top: 30rpx;
	border-radius: 10rpx;
	border: 1rpx solid #f4f4f4;
}
.navbar .nav-logo{
	width: 80rpx;
	height: 80rpx;
	margin-bottom: 30rpx;
}